<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:tint="@android:color/car_blue_100" android:shape="rectangle">
            <corners android:radius="@android:dimen/car_body2_size"/>
            <size android:height="10dp"/>
            <solid android:color="@android:color/holo_light_control_activated"/>
        </shape>
    </item>r 
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?android:attr/colorControlActivated" android:shape="rectangle">
                <corners android:radius="@android:dimen/car_body2_size"/>
                <size android:height="10dp"/>
                <solid android:color="@android:color/holo_light_control_activated"/>
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape android:tint="?android:attr/colorControlActivated" android:shape="rectangle">
                <corners android:radius="@android:dimen/car_body2_size"/>
                <size android:height="10dp"/>
                <solid android:color="@android:color/white"/>
            </shape>
        </scale>
    </item>
</layer-list>
