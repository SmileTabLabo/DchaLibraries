<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:title="@string/legal_information" android:key="legal_screen">
    <PreferenceScreen android:enabled="false" android:layout="@layout/hide_layout" android:selectable="false">
        <Preference android:title="@string/wallpaper_attributions" android:key="wallpaper_attributions" android:summary="@string/wallpaper_attributions_values"/>
    </PreferenceScreen>
    <Preference android:title="@string/copyright_title" android:key="copyright">
        <intent android:action="android.settings.COPYRIGHT"/>
    </Preference>
    <Preference android:title="@string/license_title" android:key="license">
        <intent android:action="android.settings.LICENSE"/>
    </Preference>
    <Preference android:title="@string/terms_title" android:key="terms">
        <intent android:action="android.settings.TERMS"/>
    </Preference>
    <Preference android:title="@string/webview_license_title" android:key="webview_license">
        <intent android:action="android.settings.WEBVIEW_LICENSE"/>
    </Preference>
</PreferenceScreen>
