<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto" android:title="@string/network_dashboard_title" android:key="network_and_internet_screen" settings:initialExpandedChildrenCount="5">
    <PreferenceScreen android:enabled="false" android:layout="@layout/hide_layout" android:selectable="false">
        <com.android.settingslib.RestrictedPreference android:icon="@drawable/ic_vpn_key" android:title="@string/vpn_settings_title" android:key="vpn_settings" android:summary="@string/summary_placeholder" android:order="10" android:fragment="com.android.settings.vpn2.VpnSettings" settings:useAdminDisabledSummary="true" settings:userRestriction="no_config_vpn"/>
        <com.android.settingslib.RestrictedPreference android:icon="@drawable/ic_wifi_tethering" android:title="@string/tether_settings_title_all" android:key="tether_settings" android:summary="@string/summary_placeholder" android:order="-5" android:fragment="com.android.settings.TetherSettings" settings:keywords="@string/keywords_hotspot_tethering" settings:useAdminDisabledSummary="true" settings:userRestriction="no_config_tethering"/>
        <com.android.settings.network.PrivateDnsModeDialogPreference android:title="@string/select_private_dns_configuration_title" android:key="private_dns_settings" android:order="15" android:dialogTitle="@string/select_private_dns_configuration_dialog_title" android:positiveButtonText="@string/save" android:negativeButtonText="@android:string/cancel" android:dialogLayout="@layout/private_dns_mode_dialog"/>
    </PreferenceScreen>
    <com.android.settings.widget.MasterSwitchPreference android:icon="@drawable/ic_settings_wireless" android:title="@string/wifi_settings" android:key="toggle_wifi" android:summary="@string/summary_placeholder" android:order="-30" android:fragment="com.android.settings.wifi.WifiSettings">
        <intent android:action="android.settings.WIFI_SETTINGS" android:targetClass="Settings$WifiSettingsActivity"/>
    </com.android.settings.widget.MasterSwitchPreference>
    <com.android.settingslib.RestrictedPreference android:icon="@drawable/ic_network_cell" android:title="@string/network_settings_title" android:key="mobile_network_settings" android:summary="@string/summary_placeholder" android:order="-15" settings:keywords="@string/keywords_more_mobile_networks" settings:useAdminDisabledSummary="true" settings:userRestriction="no_config_mobile_networks">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.MobileNetworkSettings"/>
    </com.android.settingslib.RestrictedPreference>
    <com.android.settingslib.RestrictedPreference android:persistent="false" android:title="@string/manage_mobile_plan_title" android:key="manage_mobile_plan" android:order="0" settings:useAdminDisabledSummary="true" settings:userRestriction="no_config_mobile_networks"/>
    <com.android.settingslib.RestrictedSwitchPreference android:icon="@drawable/ic_airplanemode_active" android:title="@string/airplane_mode" android:key="airplane_mode" android:order="5" android:disableDependentsState="true" settings:controller="com.android.settings.network.AirplaneModePreferenceController" settings:platform_slice="true" settings:userRestriction="no_airplane_mode"/>
    <Preference android:title="@string/proxy_settings_title" android:key="proxy_settings" android:fragment="com.android.settings.ProxySelector"/>
    <com.android.settingslib.RestrictedPreference android:title="@string/rcse_settings_title" android:key="rcse_settings"/>
</PreferenceScreen>
