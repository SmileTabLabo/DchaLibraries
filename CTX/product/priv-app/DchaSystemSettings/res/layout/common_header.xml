<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:id="@+id/heder_state" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/heder">
        <ImageView android:id="@+id/common_heder_state_left" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1"/>
        <ImageView android:id="@+id/common_heder_state_right" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1"/>
    </LinearLayout>
    <ImageView android:id="@+id/common_heder_detail" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/heder_state" android:layout_alignParentLeft="true"/>
</RelativeLayout>
