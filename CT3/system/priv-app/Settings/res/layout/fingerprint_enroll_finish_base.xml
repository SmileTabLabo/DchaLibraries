<?xml version="1.0" encoding="utf-8"?>
<com.android.setupwizardlib.SetupWizardLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/setup_wizard_layout" android:layout_width="match_parent" android:layout_height="match_parent" style="?attr/fingerprint_layout_theme">
    <LinearLayout android:orientation="vertical" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/SuwContentFrame">
        <TextView android:id="@+id/message" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/suw_description_margin_top" android:text="@string/security_settings_fingerprint_enroll_finish_message" style="@style/TextAppearance.FingerprintMessage"/>
        <TextView android:textColor="?android:attr/textColorSecondary" android:id="@+id/message_secondary" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/suw_description_margin_top" android:text="@string/setup_fingerprint_enroll_finish_message_secondary" style="@style/TextAppearance.FingerprintMessage"/>
        <View android:layout_width="match_parent" android:layout_height="24dp" android:layout_weight="1"/>
        <ImageView android:layout_gravity="center_horizontal" android:id="@+id/fingerprint_in_app_indicator" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/fp_app_indicator" android:maxWidth="@dimen/fingerprint_in_app_indicator_max_size" android:maxHeight="@dimen/fingerprint_in_app_indicator_max_size" android:minWidth="@dimen/fingerprint_in_app_indicator_min_size" android:minHeight="@dimen/fingerprint_in_app_indicator_min_size" android:layout_weight="5" android:contentDescription="@android:string/fingerprint_icon_content_description"/>
        <View android:layout_width="match_parent" android:layout_height="24dp" android:layout_weight="1"/>
        <LinearLayout android:layout_gravity="end" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="4dp" android:layout_marginEnd="-12dp">
            <Button android:id="@+id/add_another_button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/fingerprint_enroll_button_add" style="@style/Button.FingerprintButton"/>
            <Button android:gravity="end|center_vertical" android:id="@+id/next_button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/security_settings_fingerprint_enroll_done" style="@style/Button.FingerprintButton"/>
        </LinearLayout>
    </LinearLayout>
</com.android.setupwizardlib.SetupWizardLayout>
