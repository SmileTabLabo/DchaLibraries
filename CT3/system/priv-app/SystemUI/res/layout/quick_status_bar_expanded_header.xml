    <com.android.systemui.statusbar.AlphaOptimizedImageView android:id="@+id/qs_detail_header_progress" android:background="@color/qs_detail_progress_track" android:layout_width="match_parent" android:layout_height="wrap_content" android:src="@drawable/indeterminate_anim" android:layout_alignParentBottom="true" android:alpha="0"/>
    <TextView android:textSize="11dp" android:textStyle="bold" android:textColor="#00a040" android:layout_gravity="center_vertical" android:id="@+id/header_debug_info" android:padding="2dp" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:fontFamily="sans-serif-condensed"/>
</com.android.systemui.statusbar.phone.QuickStatusBarHeader>
