<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.statusbar.phone.QuickStatusBarHeader xmlns:android="http://schemas.android.com/apk/res/android" xmlns:systemui="http://schemas.android.com/apk/res-auto" android:layout_gravity="bottom|right" android:id="@+id/header" android:clickable="false" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="@dimen/status_bar_header_height" android:baselineAligned="false">
    <LinearLayout android:gravity="center" android:orientation="horizontal" android:clipChildren="false" android:clipToPadding="false" android:layout_width="wrap_content" android:layout_height="48dp" android:layout_marginTop="28dp" android:layout_marginEnd="12dp" android:layout_alignParentEnd="true">
        <com.android.systemui.statusbar.phone.MultiUserSwitch android:id="@+id/multi_user_switch" android:background="@drawable/ripple_drawable" android:focusable="true" android:layout_width="48dp" android:layout_height="48dp" android:layout_alignParentEnd="true">
