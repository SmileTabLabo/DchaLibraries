        </com.android.systemui.statusbar.phone.MultiUserSwitch>
        <com.android.systemui.statusbar.AlphaOptimizedFrameLayout android:id="@+id/settings_button_container" android:clipChildren="false" android:clipToPadding="false" android:layout_width="48dp" android:layout_height="48dp">
            <com.android.systemui.statusbar.phone.SettingsButton android:id="@+id/settings_button" android:background="@drawable/ripple_drawable" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/ic_settings" android:contentDescription="@string/accessibility_quick_settings_settings" style="@android:style/Widget.Material.Button.Borderless"/>
            <com.android.systemui.statusbar.AlphaOptimizedImageView android:id="@+id/tuner_icon" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/tuner" android:tint="#4dffffff" android:paddingStart="36dp" android:tintMode="src_in"/>
        </com.android.systemui.statusbar.AlphaOptimizedFrameLayout>
        <com.android.systemui.statusbar.phone.ExpandableIndicator android:id="@+id/expand_indicator" android:background="?android:attr/selectableItemBackgroundBorderless" android:padding="12dp" android:focusable="true" android:clickable="true" android:clipToPadding="false" android:layout_width="48dp" android:layout_height="48dp" android:contentDescription="@string/accessibility_quick_settings_expand"/>
