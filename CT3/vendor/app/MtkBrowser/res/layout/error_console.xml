<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/error_console_view_group_id" android:background="#000000" android:layout_width="match_parent" android:layout_height="match_parent">
